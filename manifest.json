{
  "name": "Spaces",
  "version": "0.1",
  "description": "Fast and simple tab context switcher. No frills, no logins.",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "./popup.html"
  },
  "permissions": ["tabs", "declarativeContent", "storage"],
  "manifest_version": 2
}
